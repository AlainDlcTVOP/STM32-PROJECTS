/* 6 characters print ASCII code */
# include  <stdio.h>

int main(void){

	char asci;

	printf("Enter 6 char alpha numb special\n");
	fflush(stdout);
	scanf("%c %c %c %c %c %c", &asci);

	printf("ASCII value of %c = %d %c = %d %c = %d %c = %d %c = %d %c = %d ",asci,asci);
	fflush(stdout);



return 0;
}


