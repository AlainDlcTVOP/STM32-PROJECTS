/* 6 characters print ASCII code */
# include  <stdio.h>

int main() {

	char asci [6];

	printf("Enter 6 char alpha numb special");
	fflush(stdout);
	scanf("%c %c %c %c %c %c", &asci);

	printf("ASCII value of %c = %d",asci,asci)
}


