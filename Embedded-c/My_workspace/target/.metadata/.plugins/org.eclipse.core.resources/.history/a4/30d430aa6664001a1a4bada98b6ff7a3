# include <stdio.h>

// Read input 3 integers and display the avarage

int avarge(int a,int b,int c);

int main() {
	int result,a,b,c;

 printf("Input 3 integers");
 scanf("%d%d%d",&a,&b,&c);

 printf("The result is %d", avarge(&a, &b, &c));


}
int avarge(int a,int b,int c){


	a + b + c / 3;



}



